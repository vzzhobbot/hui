<div class="hlf-input hlf-input--ac" hlf-role="input-wrap">
    <input type="text"
           placeholder="<%= placeholder %>"
           value="<%= text %>"
           tabindex="<%= tabIndex %>"
           hlf-role="input"/>
    <div class="loader" hlf-role="loader"></div>
    <div class="hint" hlf-role="hint"><%= hint %></div>
</div>